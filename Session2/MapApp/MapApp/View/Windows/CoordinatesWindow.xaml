<Window x:Class="MapApp.View.Windows.CoordinatesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MapApp.View.Windows"
        mc:Ignorable="d"
        Title="CoordinatesWindow" Height="450" Width="800">
    <StackPanel>
        <TextBox x:Name="room"/>
        <TextBox x:Name="bed"/>
        <RadioButton Content="A" x:Name="Arb"/>
        <RadioButton Content="B" x:Name="Brb"/>
        <RadioButton Content="C" x:Name="Crb"/>
        <RadioButton Content="D" x:Name="Drb"/>
        <Button IsDefault="True" Content="asfdsdafdsf" Click="Button_Click"/>
    </StackPanel>
</Window>
